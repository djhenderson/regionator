<kml xmlns="http://earth.google.com/kml/2.1">
<!--

  icon.kml

    icon: white paddle
    hotspot: anchor point at bottom middle

-->
  <Document>

    <Style id="station_icon">
      <IconStyle>
        <Icon>
          <href>wht-blank.png</href>
        </Icon>
        <hotSpot x=".5" y="0" xunits="fraction" yunits="fraction"/>
      </IconStyle>
    </Style>

<!--
  Test the Style by loading this file alone.
  This Placemark is not used when this file is referenced in
  a styleUrl in another file.
-->
    <Placemark>
      <styleUrl>#station_icon</styleUrl>
      <Point>
        <coordinates>1,1</coordinates>
      </Point>
    </Placemark>

  </Document>
</kml>
